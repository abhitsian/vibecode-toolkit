{
  "name": "vibecode-toolkit",
  "version": "0.1.0",
  "description": "A comprehensive toolkit for vibecoders to capture, debug, and communicate issues",
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "vibe": "./src/cli.ts"
  },
  "scripts": {
    "dev": "bun --hot ui/index.html",
    "build": "bun build ui/index.html --outdir dist --target browser",
    "tauri:dev": "cargo tauri dev",
    "tauri:build": "cargo tauri build",
    "cli": "bun run src/cli.ts",
    "test": "bun test"
  },
  "keywords": ["vibecode", "debugging", "developer-tools", "cli"],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@tauri-apps/api": "^2.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  }
}
